<%= simple_form_for(@group_purchase) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :event_name %>
    <%= f.input :event_info %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.association :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: 'btn btn-default' %>
  </div>
<% end %>
